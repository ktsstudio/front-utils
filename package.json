{
  "name": "@ktsstudio/mediaproject-utils",
  "version": "4.1.0",
  "author": "KTS Studio",
  "license": "MIT",
  "description": "Package with basic utils for media projects",
  "keywords": [
    "vk-mini-apps",
    "axios",
    "utils"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "rm-build": "rm -rf dist",
    "lint": "eslint src --fix",
    "codestyle": "prettier --write \"src/**/*.{ts,js}?(x)\"",
    "tsc-check": "tsc --skipLibCheck --noEmit --pretty --project tsconfig.json",
    "build": "yarn run rm-build; tsc --project tsconfig.build.json --module es6 --target es6 --outDir dist --declarationDir dist/types",
    "build:cjs": "yarn run rm-build; tsc --project tsconfig.build.json --module commonjs --target es5 --outDir dist --declarationDir dist/types",
    "precommit": "yarn run tsc-check && lint-staged",
    "test": "jest",
    "test:cover": "jest --coverage --collectCoverageFrom='src/**/*.{ts,tsx}'"
  },
  "lint-staged": {
    "*.{ts,js}?(x)": [
      "yarn eslint --quiet",
      "yarn prettier --check",
      "yarn jest --bail --findRelatedTests"
    ]
  },
  "files": [
    "dist/",
    "package.json",
    "README.md",
    "CHANGELOG.md",
    "logo.png"
  ],
  "devDependencies": {
    "@swc/core": "^1.3.49",
    "@swc/jest": "^0.2.24",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.192",
    "@types/node": "^16.4.5",
    "@types/react": "^17.0.0 || ^18.0.0",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "husky": "^7.0.1",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^11.0.0",
    "lodash": "^4.17.21",
    "prettier": "^2.5.1",
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "axios": "^0.21.1"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ktsstudio/front-utils.git"
  },
  "private": false,
  "bugs": {
    "url": "https://github.com/ktsstudio/front-utils/issues"
  },
  "homepage": "https://github.com/ktsstudio/front-utils#readme"
}
